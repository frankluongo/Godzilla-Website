<div class="container">
  <div class="m-xsmall-top-5">
    <ul>
      <% @articles.each do |article| %>
        <li>
          <%= link_to article.title, article_path(slug: article.slug) %><br>
          <p>
            <%= article.teaser %>
          </p>
        </li>
      <% end %>
    </ul>
  </div>
</div>
